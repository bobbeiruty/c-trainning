       printf("enter the redius :");
       float redius;
       scanf("%f",&redius);
       float pi=3.14;
      float red = redius*redius*redius;
      float volume= (( 1.3333333)*(pi)*(red));
       printf("The volume of sphere is:%f",volume);
    
    
    
}

